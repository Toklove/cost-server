{
  "version": "1.0",
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "envId": "{{envId}}",
  "framework": {
    "name": "go-cost-server",
    "plugins": {
      "function": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": "./",
          "functions": [
            {
              "name": "go-cost-server",
              "runtime": "Go",
              "handler": "main",
              "functionDistPath": "./main"
            }
          ]
        }
      }
    }
  }
}